{
    "name": "figlabhq/crud-resource-for-backpack",
    "description": "Build CRUD panels using fluent field definitions.",
    "license": "MIT",
    "authors": [
        {
            "name": "FigLab Team",
            "email": "hello@figlab.io",
            "homepage": "https://figlab.io"
        }
    ],
    "homepage": "https://github.com/figlabhq/crud-resource",
    "keywords": [
        "Laravel",
        "Backpack",
        "Backpack for Laravel",
        "Backpack Addon",
        "CrudResource"
    ],
    "require": {
        "illuminate/support": "^7|^8|^9",
        "backpack/crud": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "FigLab\\CrudResource\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FigLab\\CrudResource\\AddonServiceProvider"
            ]
        }
    }
}
